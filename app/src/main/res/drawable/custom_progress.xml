<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3dp" />
            <gradient
                android:centerColor="#a6a8a9"
                android:endColor="#a6a8a9"
                android:startColor="#a6a8a9" />
            <solid android:color="#a6a8a9" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dp" />
                <gradient
                    android:centerColor="#4b9ccf"
                    android:endColor="#4b9ccf"
                    android:startColor="#4b9ccf" />
                <solid android:color="#4b9ccf" />
            </shape>
        </clip>
    </item>
</layer-list>