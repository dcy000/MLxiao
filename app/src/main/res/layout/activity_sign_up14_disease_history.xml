<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cl_sign_up_root_disease_history"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bg_tab2_health_info"
    android:layout_height="match_parent"
    tools:context="com.medlink.danbogh.register.SignUp14DiseaseHistoryActivity">

    <TextView
        android:id="@+id/tv_sign_up_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="您有没有以下病史？"
        android:textColor="#8c8c8c"
        android:textSize="50sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_sign_up_content"
        android:layout_width="wrap_content"
        android:layout_height="370dp"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_sign_up_title">
    </android.support.v7.widget.RecyclerView>

    <include layout="@layout/include_sign_up_tab"/>

    <include layout="@layout/include_sign_up_nav"/>
</android.support.constraint.ConstraintLayout>
