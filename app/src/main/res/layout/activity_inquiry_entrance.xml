<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.gcml.common.widget.toolbar.TranslucentToolBar
        android:id="@+id/tb_inquiry_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="476pt"
        android:layout_marginTop="258pt"
        android:orientation="horizontal"
        android:paddingLeft="230pt"
        android:paddingRight="230pt">

        <RelativeLayout
            android:id="@+id/rl_inquiry_home_manage"
            android:layout_width="400pt"
            android:layout_height="476pt">

            <ImageView
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:background="@drawable/selector_selector_health_manager" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:text="健康管理"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_inquiry_home_file"
            android:layout_width="400pt"
            android:layout_height="476pt"
            android:layout_marginLeft="160pt">

            <ImageView
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:background="@drawable/selector_health_manager_file" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:text="建档"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_inquiry_home_home"
            android:layout_width="400pt"
            android:layout_height="476pt"
            android:layout_marginLeft="160pt">

            <ImageView
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:background="@drawable/selector_health_manager_home"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:text="主页"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>


    </LinearLayout>

    <!--<ImageView-->
    <!--android:background="#ff0"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent" />-->

</LinearLayout>
