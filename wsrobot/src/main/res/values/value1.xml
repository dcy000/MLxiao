    <string name="ws_data_loading">正在加载数据...</string>
    <string name="ws_ensure">确定</string>
    <style name="WSAppTheme" parent="WSFullScreenTheme">
    </style>
    <style name="WSAppTheme.ActionBar.Navigation" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:paddingRight">0dp</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="WSAppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/ws_bg_app</item>
        <item name="colorPrimary">@color/ws_colorPrimary</item>
        <item name="colorPrimaryDark">@color/ws_colorPrimaryDark</item>
        <item name="colorAccent">@color/ws_colorAccent</item>
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>
    <style name="WSFullScreenTheme" parent="WSAppTheme.Base">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="WSToolbar">
        <item name="android:layout_height">64dp</item>
        <item name="android:background">@color/ws_colorPrimary</item>
        <item name="theme">@style/WSToolbarTheme</item>
        <item name="popupTheme">@style/WSToolbarPopupTheme</item>
        <item name="titleTextAppearance">@style/WSToolbarTitle</item>
        <item name="contentInsetStartWithNavigation">0dp</item>
    </style>
    <style name="WSToolbarMenuText" parent="@style/TextAppearance.AppCompat.Menu">
        <item name="android:textSize">@dimen/ws_font_normal</item>
    </style>
    <style name="WSToolbarPopupTheme" parent="@style/ThemeOverlay.AppCompat">
        <item name="android:colorBackground">#212121</item>
    </style>
    <style name="WSToolbarTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="actionMenuTextColor">@color/ws_main</item>
        <item name="actionMenuTextAppearance">@style/WSToolbarMenuText</item>
    </style>
    <style name="WSToolbarTitle" parent="@style/TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/ws_font_large</item>
        <item name="android:textColor">@color/ws_textColorPrimary</item>
    </style>
    <style name="ws_radio_blue" parent="ws_txt_black_medium">
        <item name="android:button">@null</item>
        <!--<item name="android:drawableLeft">@drawable/selector_blue_check</item>-->
        <item name="android:drawablePadding">4dp</item>
        <item name="android:textColor">@color/ws_black</item>
        <item name="android:textSize">@dimen/ws_font_medium</item>
    </style>
    <style name="ws_tab_disease">
        <item name="android:textSize">@dimen/ws_font_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">8dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/ws_selector_disease_btn</item>
    </style>
