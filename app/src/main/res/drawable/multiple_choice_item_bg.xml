<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <stroke android:color="@color/colorMultipleChoiceItem"/>
            <solid android:color="@color/colorMultipleChoiceItem"/>
            <corners android:radius="50pt"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:color="@color/colorMultipleChoiceItem" android:width="4pt"/>
            <solid android:color="@color/white"/>
            <corners android:radius="50pt"/>
        </shape>
    </item>
</selector>