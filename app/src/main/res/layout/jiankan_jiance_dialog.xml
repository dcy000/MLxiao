<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1400pt"
    android:layout_height="900pt">

    <LinearLayout
        android:layout_width="1400pt"
        android:layout_height="900pt"
        android:background="@drawable/shape_bg_clear_reset_dialog"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="84pt">

            <ImageView
                android:id="@+id/jiankang_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:src="@drawable/main_dialog_jiankangtijian" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="健康体检"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="46pt">

            <ImageView
                android:id="@+id/gaoxueya_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:src="@drawable/main_dialog_xueya_tijian" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="高血压检测"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="46pt">

            <ImageView
                android:id="@+id/tangniaobing_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:src="@drawable/main_dialog_tangniaobing" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="糖尿病检测"
                android:textColor="#ff333333"
                android:textSize="64pt" />

        </RelativeLayout>


    </LinearLayout>
</FrameLayout>