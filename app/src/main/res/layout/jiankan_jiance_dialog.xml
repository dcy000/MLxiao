<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="1720pt"
    android:layout_height="900pt">

    <LinearLayout
        android:layout_width="1720pt"
        android:layout_height="900pt"
        android:background="@drawable/shape_bg_clear_reset_dialog"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="40pt">

            <ImageView
                android:id="@+id/iv_danxiang_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:background="@drawable/main_dialog_danxiangtijian_set" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="单项体检"
                android:textColor="#ff333333"
                android:textSize="64pt" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="38pt">

            <ImageView
                android:id="@+id/jiankang_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:background="@drawable/main_dialog_jiankang_tijian_set" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="健康体检"
                android:textColor="#ff333333"
                android:textSize="64pt" />

            <TextView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="160pt"
                android:text="(1次/年)"
                android:textColor="#ff5897fc"
                android:textSize="40pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="38pt">

            <ImageView
                android:id="@+id/gaoxueya_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_marginTop="213pt"
                android:background="@drawable/main_dialog_xueya_tijian_set" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="高血压随访"
                android:textColor="#ff333333"
                android:textSize="64pt" />

            <TextView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="160pt"
                android:text="(1次/2月)"
                android:textColor="#ff5897fc"
                android:textSize="40pt" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="38pt">

            <ImageView
                android:id="@+id/tangniaobing_tijian"
                android:layout_width="380pt"
                android:layout_height="380pt"
                android:layout_alignBottom="@+id/textView14"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="true"
                android:layout_marginBottom="30dp"
                android:background="@drawable/main_dialog_tangniaobing_set" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="213pt"
                android:text="糖尿病随访"
                android:textColor="#ff333333"
                android:textSize="64pt" />

            <TextView
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="160pt"
                android:text="(1次/2月)"
                android:textColor="#ff5897fc"
                android:textSize="40pt" />


        </RelativeLayout>


    </LinearLayout>
</FrameLayout>