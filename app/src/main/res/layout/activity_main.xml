<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_main"
    tools:context="com.example.han.referralproject.MainActivity">

    <ImageView
        android:id="@+id/health_test"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="20dp"
        android:clickable="true"
        android:src="@drawable/health_test" />

    <ImageView
        android:id="@+id/doctor_remind"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginLeft="580dp"
        android:layout_marginTop="20dp"
        android:clickable="true"
        android:src="@drawable/doctor_remind" />

    <ImageView
        android:id="@+id/person_info"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="240dp"
        android:clickable="true"
        android:src="@drawable/person_info" />

    <ImageView
        android:id="@+id/face_to_face"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginLeft="640dp"
        android:layout_marginTop="240dp"
        android:clickable="true"
        android:src="@drawable/face_to_face" />

    <ImageView
        android:id="@+id/robot_conversion"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginLeft="450dp"
        android:layout_marginTop="400dp"
        android:clickable="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/robot_conversion" />



</RelativeLayout>
