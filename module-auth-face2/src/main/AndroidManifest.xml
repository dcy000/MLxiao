<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gcml.auth.face2">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />

    <application>
        <activity android:name=".ui.FaceSignUpActivity" />
        <activity android:name=".ui.FaceSignInActivity" />
        <activity android:name=".ui.FaceBdSignUpActivity" />
        <activity android:name=".ui.FaceBdSignInActivity" />
        <activity android:name=".ui.FaceBdVerifyActivity"></activity>
    </application>

</manifest>