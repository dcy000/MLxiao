    <declare-styleable name="WSFlowLayout"><attr name="android:gravity"/></declare-styleable>
    <declare-styleable name="WSFlowLayout_Layout"><attr name="android:layout_gravity"/></declare-styleable>
    <declare-styleable name="WSPageFlowView"><attr format="dimension" name="ws_indicatorMargin"/><attr format="integer" name="ws_maxRows"/><attr format="integer" name="ws_maxPages"/></declare-styleable>
    <declare-styleable name="WSWheelPicker"><attr format="color" name="ws_normalTextColor"/><attr format="dimension" name="ws_normalTextSize"/><attr format="color" name="ws_selecredTextColor"/><attr format="dimension" name="ws_selecredTextSize"/><attr format="dimension" name="ws_unitHight"/><attr format="integer" name="ws_itemNumber"/><attr format="color" name="ws_lineColor"/><attr format="dimension" name="ws_maskHight"/><attr format="boolean" name="ws_noEmpty"/><attr format="boolean" name="ws_isEnable"/></declare-styleable>
    <dimen name="ws_activity_horizontal_margin">16dp</dimen>
    <dimen name="ws_activity_vertical_margin">16dp</dimen>
    <dimen name="ws_font_huge">38sp</dimen>
    <dimen name="ws_font_huger">42sp</dimen>
